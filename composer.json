{
    "name": "zaxxo/laravel-bruteforce",
    "description": "This package adds a simple bruteforce protection to Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zaxxo",
            "email": "info@zaxxo.de"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/console": "^5.5|^6",
        "illuminate/database": "^5.5|^6",
        "illuminate/http": "^5.5|^6",
        "illuminate/routing": "^5.5|^6",
        "illuminate/support": "^5.5|^6"
    },
    "autoload": {
        "psr-4": {
